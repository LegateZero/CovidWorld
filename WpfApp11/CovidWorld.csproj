    <PackageReference Include="OxyPlot.Wpf" Version="2.0.0" />
    <PackageReference Include="XAML.MapControl" Version="7.2.1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Infrastructure\Commands\Base\" />
